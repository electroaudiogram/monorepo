{
  "name": "@electroaudiogram/ui",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "./dist"
  ],
  "publishConfig": {
    "exports": {
      ".": {
        "import": {
          "default": "./dist/index.mjs",
          "import": "./dist/index.mjs",
          "require": "./dist/index.js",
          "types": "./dist/index.d.ts"
        }
      }
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest --coverage"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@electroaudiogram/typescript-config": "workspace:*",
    "@vitest/coverage-v8": "^3.2.4",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
